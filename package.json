{
  "name": "vue-cryptobar",
  "version": "1.1.1-daily.20",
  "private": false,
  "description": "A dynamic cryptocurrency module which looks modern and clear.",
  "author": "Onur OZKAN",
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build",
    "e2e": "vue-cli-service e2e",
    "lint": "vue-cli-service lint",
    "build-bundle": "vue-cli-service build --target lib --name cryptobar ./src/components/cryptobar.vue",
    "e2e:open": "vue-cli-service e2e:open"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "cryptocompare": "^0.7.0",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.1.1",
    "@vue/cli-plugin-eslint": "^3.2.1",
    "@vue/cli-service": "^3.1.4",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "eslint-plugin-html": "^5.0.0",
    "node": "^11.2.0",
    "vue-template-compiler": "^2.5.17",
    "webpack-cli": "^3.1.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/standard"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "browser": {
    "./sfc": "dist/vue-cryptobar.vue"
  },
  "keywords": [
    "vue-cryptobar",
    "vue-crypto",
    "cryptobar",
    "crypto",
    "cryptocurrency",
    "currency",
    "bitcoin",
    "ethereum",
    "api",
    "price"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "peerDependencies": {
    "vue": "2.x"
  },
  "repository": "git://github.com/onurrozkaan/vue-cryptobar.git",
  "unpkg": "dist/vue-cryptobar.js"
}
